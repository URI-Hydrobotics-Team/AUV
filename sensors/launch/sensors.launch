<launch>
    <node pkg="echosounder" type="echosounder.py" name="echosounder" output="screen"></node>

    <include file = "include/bno055.launch">
        <arg name = "node_name" value = "front_echosounder"/>
        <arg name = "port" value = "/dev/ttyAMA0"/>
    </include>

    <include file = "include/echosounder.launch">
        <arg name = "node_name" value = "down_echosounder"/>
        <arg name = "port" value = "/dev/ttyAMA1"/>
    </include>
</launch>

